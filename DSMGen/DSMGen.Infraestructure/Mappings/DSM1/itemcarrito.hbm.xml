<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DSMGen.Infraestructure"
		                   namespace="DSMGen.Infraestructure.EN.DSM1">
		    
		    
			
				<class name="ItemCarritoNH" table="ItemCarrito"> 
						<id name="Cantidad" column="cantidadItemCarrito"/> 
			
					
					<property name="Subtotal" type="double"> 
						<column name="subtotal"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<many-to-one name="Carrito" class="DSMGen.Infraestructure.EN.DSM1.CarritoNH" >	
								<column name="FK_idCarritoCarrito_idCarrito"/> 
						</many-to-one>
											 					
			
		  			
					
							<many-to-one name="Sombrero" class="DSMGen.Infraestructure.EN.DSM1.SombreroNH" >
								<column name="FK_idSombreroSombrero_idSombrero"/> 
        					</many-to-one>
					 
						
					
					
					
					
		
		 </class>	
		  
		  <query name="ItemCarritoNHReadAllHQL">from ItemCarritoNH</query>
		</hibernate-mapping>
	